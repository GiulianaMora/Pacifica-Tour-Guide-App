<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.pacificatourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BeachesActivity"
            android:label="@string/category_beaches" />
        <activity
            android:name=".ParksTrailsActivity"
            android:label="@string/category_parks_trails" />
        <activity
            android:name=".RestaurantsActivity"
            android:label="@string/category_restaurants" />
        <activity
            android:name=".RadSpotsActivity"
            android:label="@string/category_rad_spots" />
    </application>

</manifest>